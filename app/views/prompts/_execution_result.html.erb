<div class="border rounded-md p-4 mt-4 <%= execution.status == 'completed' ? 'border-green-200 bg-green-50' : 'border-red-200 bg-red-50' %>">
  <div class="flex items-center justify-between mb-2">
    <span class="text-sm font-medium <%= execution.status == 'completed' ? 'text-green-700' : 'text-red-700' %>">
      <%= execution.status == 'completed' ? '실행 완료' : '실행 실패' %>
    </span>
    <span class="text-xs text-gray-500">
      <%= execution.duration_ms %>ms · <%= execution.token_count %> tokens
    </span>
  </div>
  <div class="bg-white rounded-md p-3 text-sm text-gray-800 whitespace-pre-wrap font-mono max-h-96 overflow-y-auto"><%= execution.response_text %></div>
</div>
